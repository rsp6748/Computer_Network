#include<stdio.h>
#include<conio.h>

int main ()
{
    int input, q,output,token,left;
    token=10;
    printf("enter the queries:");
    scanf("%d",&q);
 for(int i=0;i<q;i++)
    {  printf ("\n enter the input :");
        scanf("%d",&input);
      if(input<=token)
       {
           left=token-input;
          printf("token remaining %d",left);
         output=input;
           printf("data passed %d",output);
          
       }

    else     
         { 
         output=input-token;
          printf("token remaining:0");
          printf("passed token %d", token);
          printf("left packet %d", output);
         }
     
     token= left+10;
     
    }
    
}
