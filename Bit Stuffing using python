data=list(input("Ã«nter the data:" ))
i=0
count=0
while i<len(data):
    if (data[i]=='1'):
        count=count+1
        if (count==5):
            data.insert(i+1, '0')
            i=i+2
            count=0
    else:
        count=0
    i=i+1

listToStr = ' '.join([str(elem) for elem in data])
 
print(listToStr)
